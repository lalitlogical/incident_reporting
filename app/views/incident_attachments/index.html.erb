<p id="notice"><%= notice %></p>

<h1>Incident Attachments</h1>

<table>
  <thead>
    <tr>
      <th>Incident</th>
      <th>Avatar</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @incident_attachments.each do |incident_attachment| %>
      <tr>
        <td><%= incident_attachment.incident_id %></td>
        <td><%= incident_attachment.avatar %></td>
        <td><%= link_to 'Show', incident_attachment %></td>
        <td><%= link_to 'Edit', edit_incident_attachment_path(incident_attachment) %></td>
        <td><%= link_to 'Destroy', incident_attachment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Incident Attachment', new_incident_attachment_path %>
